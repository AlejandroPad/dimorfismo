            "titulo": "Morfometría de Albatros de Laysan en Isla Guadalupe",
            "schema": {
                "fields": [
                    {
                        "name": "Fecha",
                        "axis": "T",
                        "long_name": "Year",
                        "nombre_largo": "Año",
                        "standard_name": "date",
                        "type": "integer",
                        "constraints": {
                            "minumum": 2015, "maximum": 2018
                        }
                    },
                    {
                        "name": "Subcolonia",
                        "long_name": "Subcolony",
                        "nombre_largo": "Subcolonia",
                        "type": "string",
                        "constraints": {
                            "enum": [
                                "Punta Sur",
                                "Zapato",
                                "Morro Prieto"
                            ]
                        }
                    },
                    {
                        "name": "NidoPS",
                        "long_name": "Nest",
                        "nombre_largo": "Nido",
                        "type": "integer",
                        "constraints": {
                            "minumum": 1 ,"maximum": 355
                        }
                    },
                    {
                        "name": "Darvic",
                        "long_name": "Darvic",
                        "nombre_largo": "Darvic",
                        "type": "string"
                    },
                    {
                        "name": "Sexo",
                        "long_name": "Sex",
                        "nombre_largo": "Sexo",
                        "type": "string",
                        "constraints": {
                            "enum": [
                                "H",
                                "M"
                            ]
                        }
                    },
                    {
                        "name": "longitudCraneo",
                        "long_name": "Skull length",
                        "nombre_largo": "Longitud del cráneo",
                        "type": "number",
                        "units": "mm",
                        "constraints": {
                            "minimum": 165.58 ,"maximum": 193.22
                        }
                    },
                    {
                        "name": "longitudPico",
                        "long_name": "Beak length",
                        "nombre_largo": "Longitud del pico",
