                    },
                    {
                        "name": "longitudNarina",
                        "long_name": "Nare length",
                        "nombre_largo": "Longitud de la narina",
                        "type": "number",
                        "units": "mm",
                        "constraints": {
                            "minimum": 74.82 ,"maximum": 90.68
                        }
                    },
                    {
                        "name": "anchoCraneo",
                        "long_name": "Skull width",
                        "nombre_largo": "Ancho del cráneo",
                        "type": "number",
                        "units": "mm",
                        "constraints": {
                            "minimum": 46.5, "maximum": 58.36
                        }
                    },
                    {
                        "name": "altoPico",
                        "long_name": "Beak height",
                        "nombre_largo": "Alto del pico",
                        "type": "number",
                        "units": "mm",
                        "constraints": {
                            "minimum": 28.85, "maximum": 35.5
                        }
                    },
                    {
                        "name": "anchoPico",
                        "long_name": "Beak width",
                        "nombre_largo": "Ancho del pico",
                        "type": "number",
                        "units": "mm",
                        "constraints": {
                            "minimum": 21.48, "maximum": 28.7
                        }
                    },
                    {
                        "name": "Tarso",
                        "long_name": "Tarsus",
                        "nombre_largo": "Tarso",
                        "type": "number",
                        "units": "mm",
                        "constraints": {
                            "minimum": 83.18, "maximum": 99.69
                        }
                    },
                    {
                        "name": "longAlaCerrada",
                        "long_name": "Closed wing length",
                        "nombre_largo": "Longitud del ala cerrada",
                        "type": "number",
                        "units": "cm",
                        "constraints": {
                            "minimum": 48.7, "maximum": 54.7
                        }
                    },
                    {
                        "name": "longAlaAbierta",
                        "long_name": "Opened wing length",
                        "nombre_largo": "Longitud del ala abierta",
                        "type": "number",
                        "units": "cm",
                        "constraints": {
                            "minimum": 88.9, "maximum": 102
                        }
                    },
                    {
                        "name": "mediaEnvergadura",
                        "long_name": "Half wingspan",
                        "nombre_largo": "Media envergadura",
                        "type": "number",
                        "units": "cm",
                        "constraints": {
                            "minimum": 99.3, "maximum": 112.2
                        }
                    },
                    {
                        "name": "Envergadura",
                        "long_name": "Wingspan",
                        "nombre_largo": "Envergadura",
                        "type": "number",
                        "units": "cm",
                        "constraints": {
                            "minimum": 198.6, "maximum": 224.4
                        }
                    },
                    {
                        "name": "Peso",
                        "long_name": "Weight",
                        "nombre_largo": "Peso",
                        "type": "number",
                        "units": "kg",
                        "constraints": {
                            "minimum": 2.03, "maximum": 3.68
                        }
                    }
                ],
                "missingValues": [
                    "NA"
                ]
            }
