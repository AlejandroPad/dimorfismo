{
    "name": "morfometria-albatros-laysan-guadalupe",
    "homepage": "https://bitbucket.org/IslasGECI/datos-texto/src/default/datapackage/morfometria-albatros-laysan-guadalupe/",
    "profile": "tabular-data-package",
    "contributors": [
        {
            "title": "Mariana Salinas",
            "email": "mariana.salinas@islas.org.mx",
            "organization": "GECI",
            "role": "author"
        },
        {
            "title": "Yutzil Lora",
            "email": "yutzil.lora@islas.org.mx",
            "organization": "GECI",
            "role": "contributor"
        },
        {
            "title": "Haran Peiro-Nuño",
            "email": "hpeiro@uabc.edu.mx",
            "organization": "GECI",
            "role": "maintainer"
        }
    ],
    "description": "Morfometría de albatros de Laysan en Isla Guadalupe (2015-2016) para determinar el dimorfismo sexual de esta especie mediante regresión logística.",
    "institution": "Grupo de Ecología y Conservación de Islas",
    "sources": [
        {
            "title": "Julio Hernández Montoya",
            "email": "julio.montoya@islas.org.mx"
        }
    ],
    "resources": [
        {
            "name": "morfometria_albatros-laysan_guadalupe",
            "description": "Morfometría de albatros de Laysan en Isla Guadalupe (2015-2016)",
            "encoding": "utf-8",
            "format": "csv",
            "mediatype": "text/csv",
            "path": "laysan_albatross_morphometry_guadalupe.csv",
            "profile": "tabular-data-resource",
            "source": "Medidas in situ",
            "title": "Laysan Albatross morphometry from Guadalupe Island",
            "titulo": "Morfometría de Albatros de Laysan en Isla Guadalupe",
            "schema": {
                "fields": [
                    {
                        "name": "Temporada",
                        "axis": "T",
                        "long_name": "Year",
                        "nombre_largo": "Año",
                        "standard_name": "date",
                        "type": "integer",
                        "description": "Año de periodo de monitoreo durante temporada de reproducción de Albatros Laysan",
                        "constraints": {
                            "minumum": 2015,
                            "maximum": 2018
                        }
                    },
                    {
                        "name": "subcolonia",
                        "long_name": "Subcolony",
                        "nombre_largo": "Subcolonia",
                        "type": "string",
                        "description": "Colonia de distrubución interna de Isla Guadalupe",
                        "constraints": {
                            "enum": [
                                "Punta Sur",
                                "Zapato",
                                "Morro Prieto"
                            ]
                        }
                    },
                    {
                        "name": "id_nido",
                        "long_name": "Nest",
                        "nombre_largo": "Nido",
                        "type": "integer",
                        "description": "Código de identificación de refugios de Aves Laysan",
                        "constraints": {
                            "minumum": 1,
                            "maximum": 355
                        }
                    },
                    {
                        "name": "id_darvic",
                        "long_name": "Darvic",
                        "nombre_largo": "Darvic",
                        "type": "string",
                        "description": "Código de identificación representado en el anillo visible de cada Ave Laysan",
                        "constraints": {
                            "pattern": "[0-9|A-Z]{3}|\\d+"
                        }
                    },
                    {
                        "name": "sexo",
                        "long_name": "Sex",
                        "nombre_largo": "Sexo",
                        "type": "string",
                        "description": "Sexo del Ave Laysan extraído por medio sanguineo donde H representa a hembras y M a machos",
                        "constraints": {
                            "enum": [
                                "H",
                                "M"
                            ]
                        }
                    },
                    {
                        "name": "longitud_craneo",
                        "long_name": "Cranial length",
                        "nombre_largo": "Longitud del cráneo",
                        "type": "number",
                        "units": "mm",
                        "description": "Distancia en milímetros entre la punta del pico hasta el curvatura craneal respecto al plano Sagital del Ave",
                        "constraints": {
                            "minimum": 165.57,
                            "maximum": 193.23
                        }
                    },
                    {
                        "name": "longitud_pico",
                        "long_name": "Bill length",
                        "nombre_largo": "Longitud del pico",
                        "type": "number",
                        "units": "mm",
                        "description": "Distancia en milímetros de longitud de extremo a extremo del pico respecto al plano Sagital del Ave",
                        "constraints": {
                            "minimum": 100.8,
                            "maximum": 119.49
                        }
                    },
                    {
                        "name": "longitud_narina",
                        "long_name": "Nare length",
                        "nombre_largo": "Longitud de la narina",
                        "type": "number",
                        "units": "mm",
                        "description": "Distancia en milímetros del pico hasta los orificios de las fosas nasales respecto al plano Sagital del Ave",
                        "constraints": {
                            "minimum": 74.81,
                            "maximum": 90.7
                        }
                    },
                    {
                        "name": "ancho_craneo",
                        "long_name": "Craneal width",
                        "nombre_largo": "Ancho del cráneo",
                        "type": "number",
                        "units": "mm",
                        "description": "Distancia mayor en milímetros entre extremos del craneo respecto al plano Coronal del Ave",
                        "constraints": {
                            "minimum": 46.4,
                            "maximum": 58.7
                        }
                    },
                    {
                        "name": "altura_pico",
                        "long_name": "Bill height",
                        "nombre_largo": "Alto del pico",
                        "type": "number",
                        "units": "mm",
                        "description": "Distancia del extremo inferior izquierdo al superior del pico pasando por las narinas respecto al plano Sagital del Ave",
                        "constraints": {
                            "minimum": 28.84,
                            "maximum": 35.51
                        }
                    },
                    {
                        "name": "ancho_pico",
                        "long_name": "Bill width",
                        "nombre_largo": "Ancho del pico",
                        "type": "number",
                        "units": "mm",
                        "description": "Distancia en milímetros de una fosa nasal hacia la otra respecto al plano Coronal del Ave",
                        "constraints": {
                            "minimum": 21.47,
                            "maximum": 28.82
                        }
                    },
                    {
                        "name": "tarso",
                        "long_name": "Tarsus",
                        "nombre_largo": "Tarso",
                        "type": "number",
                        "units": "mm",
                        "description": "Distancia en milímetros de extremo a extremo del tarsus respecto al plano Coronal del Ave",
                        "constraints": {
                            "minimum": 83.17,
                            "maximum": 99.7
                        }
                    },
                    {
                        "name": "longitud_ala_cerrada",
                        "long_name": "Closed wing length",
                        "nombre_largo": "Longitud del ala cerrada",
                        "type": "number",
                        "units": "cm",
                        "description": "Longitud en centímetros de las remerias primarias respecto al plano Coronal del Ave ",
                        "constraints": {
                            "minimum": 48.6,
                            "maximum": 54.8
                        }
                    },
                    {
                        "name": "longitud_ala_abierta",
                        "long_name": "Opened wing length",
                        "nombre_largo": "Longitud del ala abierta",
                        "type": "number",
                        "units": "cm",
                        "description":"Longitud en centímetros de extremo a extremo del Ala completamente extendida respecto al plano Coronal del Ave",
                        "constraints": {
                            "minimum": 88.8,
                            "maximum": 102.1
                        }
                    },
                    {
                        "name": "media_envergadura",
                        "long_name": "Half wingspan",
                        "nombre_largo": "Media envergadura",
                        "type": "number",
                        "units": "cm",
                        "description": "Distancia en centímetros entre un extremo del ala hasta el centro del pecho respecto al plano Coronal del Ave",
                        "constraints": {
                            "minimum": 99.2,
                            "maximum": 112.3
                        }
                    },
                    {
                        "name": "envergadura",
                        "long_name": "Wingspan",
                        "nombre_largo": "Envergadura",
                        "type": "number",
                        "units": "cm",
                        "description": "Distancia en centímetros entre las dos puntas de las alas cuando están completamente extendidas respecto al plano Coronal del Ave",
                        "constraints": {
                            "minimum": 198.5,
                            "maximum": 224.5
                        }
                    },
                    {
                        "name": "peso",
                        "long_name": "Weight",
                        "nombre_largo": "Peso",
                        "type": "number",
                        "units": "kg",
                        "description": "Peso individual del Ave",
                        "constraints": {
                            "minimum": 2.02,
                            "maximum": 3.69
                        }
                    },
                    {
                        "name": "notas",
                        "long_name": "Notes",
                        "nombre_largo": "Notas",
                        "type": "string",
                        "description": "Información revelante y adional proporcionada "
                    }
                ],
                "missingValues": [
                    "NA"
                ]
            }
        }
    ]
}